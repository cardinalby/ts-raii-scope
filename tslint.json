{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-empty": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "variable-name": [true, "check-format", "allow-leading-underscore"],
    "no-reference": false,
    "no-namespace": false,
    "no-console": false,
    "only-arrow-functions": false
  }
}